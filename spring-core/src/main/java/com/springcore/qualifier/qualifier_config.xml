<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        
        http://www.springframework.org/schema/context 
        http://www.springframework.org/schema/context/spring-context.xsd">

	<!-- bean definitions here -->
	<context:annotation-config />
	
	<bean name="addr1" class="com.springcore.qualifier.Address">
		<property name="street" value="x-street" />
		<property name="city" value="Hyderabad" />
	</bean>

	<bean name="addr2" class="com.springcore.qualifier.Address">
		<property name="street" value="xz-street fS" />
		<property name="city" value="Pune" />
	</bean> 
	
	<bean name="emp" class="com.springcore.qualifier.Employee"></bean>
</beans>
